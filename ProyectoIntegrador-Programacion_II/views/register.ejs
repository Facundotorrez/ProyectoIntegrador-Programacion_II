<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->
<%- include('./partials/head') %> 
<body>
	
   <%- include('./partials/header') %> 

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Registrate</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<h2 class="col-12"> 
				<form action="login.ejs" method="get">
					<label for="">Nombre</label>
					<input type="text" name="nombre-usuario" id="usuario" placeholder="Ej:Facundo">
					<label for="">Email</label>
					<input for="email" name="mail" placeholder="Ej: jose@gmail.com">
					<label for="">Contraseña</label>
					<input type="password" name="contraseña" id="contraseña" placeholder="Ej: Sushi123">
					<label for="">Fecha de nacimiento</label>
					<input type="date" name="birth-date" id="birth-date" placeholder="Ej: 09/09/2001">
					<label for="">DNI</label>
					<input type="number" name="DNI" id="DNI" placeholder="Ej: 43780871">
					<label for="">Foto de perfil</label>
					<input type="file" name="choose-file" id="imgen">
					
				</form>
				
			</h2>


	<%- include('./partials/footer') %> 
</body>

</html>